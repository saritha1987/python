def linear_search(arr, target):
    for element in arr:
        if element == target:
            return True
    return False
arr = [2, 4, 6, 8, 9, 7, 9]
target1 = 7
target2 = 5
result1 = "Exist" if linear_search(arr, target1) else "Not exist"
result2 = "Exist" if linear_search(arr, target2) else "Not exist"
print(f"Target: {target1}\nResult: {result1}")
print(f"Target: {target2}\nResult: {result2}")

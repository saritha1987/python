def is_prime(n):
    if n <= 1:
        return False
    if n == 2:
        return True
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True
def find_primes(arr):
    return [num for num in arr if is_prime(num)]
array = [100, 100, 100, 100, 100, 100, 100, 100]
prime_numbers = find_primes(array)
print("Prime numbers in Array elements =", prime_numbers)

